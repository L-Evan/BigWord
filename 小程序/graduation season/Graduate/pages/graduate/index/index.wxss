.contentWarp {  
    position: absolute;  
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto; 
    /* overflow: hidden; */
    /* background-color: #d1e3f1; */
}

.touchWrap {
    transform-origin: center;
    position: absolute;
    z-index: 100;
}

.imgWrap {
    box-sizing: border-box;
    width: 100%;
    transform-origin: center;
    float: left;
    border: 0.5px transparent dashed;
}

.imgWrap image {
    float: left;
}

.touchActive .x {
    display: block;
}

.touchActive .o {
    display: block;
}

.add {
    width: 5vw;
    height: 5vw;
    position: absolute;
    z-index: 0;
}

.x {
    position: absolute;
    top: -12.5px;
    left: -12.5px;
    z-index: 500;
    display: none;
    width: 20px;
    height: 20px;
    overflow: hidden;
    font-weight: bold;
    color: #d1e3f1;
}

.o {
    position: absolute;
    bottom: -12.5px;
    right: -12.5px;
    width: 20px;
    height: 20px;
    text-align: center;
    display: none;
    overflow: hidden;
    font-weight: bold;
    color: #d1e3f1;
}

.active {
    background-color: rgb(78, 114, 151);
}

.active view {
    border: none;
}

.touchActive {
    /* border: 4rpx #fff dashed; */
    z-index: 400;
}


