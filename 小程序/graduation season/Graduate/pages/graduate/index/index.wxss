/**index.wxss**/
page{
    overflow: scroll;
    animation: noscroll 5s;
}
@keyframes noscroll{ 
    0%{overflow: hidden;}
    100%{overflow: hidden;}
  }
/* 开场动画样式 */
.startshow{
    width:100vw;
    height:100vh;
    background-color: #fff;
    opacity: 0;
    animation: showandfade 5s ease;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  
  @keyframes showandfade{ /*设置内容由显示变为隐藏*/
    0%{opacity: 0;}
    50%{opacity: 1;}
    100%{opacity: 0;}
  }

  .remind-box {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 300rpx;
  }
  
  .remind-img {
    width: 250rpx;
    height: 250rpx;
    padding-bottom: 25rpx;
  }
  .content{
    opacity: 0;
    animation: show 2s ease;
    animation-delay: 5s;
  }
  @keyframes show{ /*设置内容由显示变为隐藏*/
    0%{opacity: 0;}
    100%{opacity: 1;}
  }

